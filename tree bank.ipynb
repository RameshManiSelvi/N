   "source": [
    "import nltk\n",
    "from nltk.corpus import treebank\n",
    "from nltk import ne_chunk\n",
    "\n",
    "nltk.download('treebank')\n",
    "\n",
    "sentence = treebank.sents()[0]\n",
    "\n",
    "pos_tags = nltk.pos_tag(sentence)\n",
    "\n",
    "ner_tags = ne_chunk(pos_tags)\n",
    "\n",
    "ner_tags.draw()\n"
   ]
