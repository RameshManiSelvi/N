   "source": [
    "import spacy\n",
    "\n",
    "nlp = spacy.load(\"en_core_web_sm\")\n",
    "\n",
    "def perform_pos_tagging(text):\n",
    "    doc = nlp(text) \n",
    "    for token in doc:\n",
    "        print(f\"Token: {token.text}, POS tag: {token.pos_}\")\n",
    "\n",
    "text = \"I saw a black cat crossing the road.\"\n",
    "perform_pos_tagging(text)"
   ]
