    "import re\n",
    "\n",
    "input_string = input(\"Enter a string: \")\n",
    "\n",
    "pattern = r\"101+\"\n",
    "\n",
    "match = re.fullmatch(pattern, input_string)\n",
    "\n",
    "if match:\n",
    "    print(\"Input string matches the FA pattern.\")\n",
    "else:\n",
    "    print(\"Input string does not match the FA pattern.\")\n"
   ]
